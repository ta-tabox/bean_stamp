<%= form_with(model: [current_user.wants.find_by(offer_id: offer.id)],
    html: { method: :delete }, local: true) do |f| %>
  <% if offer.ended_at >= Date.current %>
    <%= f.submit "Unwant", class: "btn btn-primary" %>
  <% else %>
    <%= f.submit "Unwant", disabled: 'disabled', class: "btn btn-disabled" %>
  <% end %>
<% end %>
