<%= form_with( model: want, url: rate_want_path, method: "patch", local: false) do |f| %>
  <% Want.rates.each do |rate| %>
    <% next if rate[0] == 'unrated' %>
    <%= f.label rate[0] %>
    <%= f.radio_button :rate, rate[0] %>
  <% end %>
  <%= f.submit "評価する", class: "btn btn-primary" %>
<% end %>
